{
  "name": "frontend-take-home-assignment",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ng build",
    "e2e": "ng e2e",
    "format": "prettier --write \"./{src,e2e}/**/*.{md,html,ts,js,json,scss}\"",
    "format:check": "prettier --list-different \"./{src,e2e}/**/*.{md,html,ts,js,json,scss}\"",
    "lint": "ng lint && stylelint \"./src/**/*.scss\" --syntax scss",
    "lint:fix": "ng lint --fix=true && stylelint \"./src/**/*.scss\" --syntax scss --fix",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test --watch=false --code-coverage",
    "test:dev": "ng test --code-coverage --browsers=Chrome"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.scss": "stylelint --syntax scss --fix",
    "{src,e2e}/**/*.{md,html,ts,js,json,scss}": "prettier --write"
  },
  "dependencies": {
    "@angular/animations": "~10.1.2",
    "@angular/common": "~10.1.2",
    "@angular/compiler": "~10.1.2",
    "@angular/core": "~10.1.2",
    "@angular/forms": "~10.1.2",
    "@angular/platform-browser": "~10.1.2",
    "@angular/platform-browser-dynamic": "~10.1.2",
    "@angular/router": "~10.1.2",
    "ngx-mask": "^10.0.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.1",
    "zone.js": "~0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.2",
    "@angular/cli": "~10.1.2",
    "@angular/compiler-cli": "~10.1.2",
    "@types/jasmine": "~3.5.14",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.11.1",
    "codelyzer": "^6.0.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-junit-reporter": "^2.0.1",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "puppeteer": "^5.3.0",
    "stylelint": "^13.7.1",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-scss": "^3.18.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.0.3"
  }
}
